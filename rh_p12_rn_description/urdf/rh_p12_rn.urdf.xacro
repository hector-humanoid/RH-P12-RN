<?xml version="1.0"?>
<robot name="rh_p12_rn" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- some parameters -->
  <xacro:macro name="rh_p12_rn_macro" params="parent prefix reflect *origin">

    <joint name="${prefix}_rh_p12_rn_joint" type="fixed">
      <xacro:insert_block name="origin"/>
      <parent link="${parent}"/>
      <child link="${prefix}_rh_p12_rn_base"/>
    </joint>

    <!-- base -->
    <link name="${prefix}_rh_p12_rn_base">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/base.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/base.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 0.000 0.032" rpy="0 0 0"/>
        <mass value="0.236"/>
        <!--<inertia ixx="0.00017" ixy="0.0" ixz="0.0" iyy="0.00011" iyz="0.0" izz="0.00017" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>

    <gazebo reference="${prefix}_rh_p12_rn_base">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
      <material>Gazebo/Grey</material>
    </gazebo>

    <!-- Joint -->
    <joint name="${prefix}_rh_p12_rn" type="revolute">
      <parent link="${prefix}_rh_p12_rn_base"/>
      <child link="${prefix}_rh_p12_rn_r1"/>
      <origin xyz="0.0 0.008 0.048" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.1" />
      <dynamics damping="0.7"/>
    </joint>

    <transmission name="${prefix}_tran_r1">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}_rh_p12_rn">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="${prefix}_motor_r1">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- rh_p12_rn_r1 -->
    <link name="${prefix}_rh_p12_rn_r1">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/r1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/r1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 0.034 0.004" rpy="0 0 0"/>
        <mass value="0.068"/>
        <!--<inertia ixx="0.00004" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="-0.00001" izz="0.00002" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>

    <gazebo reference="${prefix}_rh_p12_rn_r1">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
      <material>Gazebo/Grey</material>
    </gazebo>

    <!-- Right Joint 2 -->
    <joint name="${prefix}_rh_r2" type="revolute">
      <parent link="${prefix}_rh_p12_rn_r1"/>
      <child link="${prefix}_rh_p12_rn_r2"/>
      <origin xyz="0.0 0.0493634 0.0285" rpy="0 0 0"/>
      <axis xyz="-1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.0" />
      <dynamics damping="0.7"/>
    </joint>

    <transmission name="${prefix}_tran_r2">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}_rh_r2">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="${prefix}_motor_r2">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- rh_p12_rn_r2 -->
    <link name="${prefix}_rh_p12_rn_r2">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/r2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/r2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 0.006 0.011" rpy="0 0 0"/>
        <mass value="0.022"/>
        <!--<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.0" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>

    <gazebo reference="${prefix}_rh_p12_rn_r2">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
      <material>Gazebo/Grey</material>
    </gazebo>

    <!-- Left Joint 1 -->
    <joint name="${prefix}_rh_l1" type="revolute">
      <parent link="${prefix}_rh_p12_rn_base"/>
      <child link="${prefix}_rh_p12_rn_l1"/>
      <origin xyz="0.0 -0.008 0.048" rpy="0 0 0"/>
      <axis xyz="-1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.1" />
      <dynamics damping="0.7"/>
    </joint>

    <transmission name="${prefix}_tran_l1">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}_rh_l1">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="${prefix}_motor_l1">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- rh_p12_rn_l1 -->
    <link name="${prefix}_rh_p12_rn_l1">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/l1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/l1.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 -0.034 0.004" rpy="0 0 0"/>
        <mass value="0.068"/>
        <!--<inertia ixx="0.00004" ixy="0.0" ixz="0.0" iyy="0.00002" iyz="0.00001" izz="0.00002" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>

    <gazebo reference="${prefix}_rh_p12_rn_l1">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
      <material>Gazebo/Grey</material>
    </gazebo>

    <!-- Left Joint 2 -->
    <joint name="${prefix}_rh_l2" type="revolute">
      <parent link="${prefix}_rh_p12_rn_l1"/>
      <child link="${prefix}_rh_p12_rn_l2"/>
      <origin xyz="0.0 -0.0493634 0.0285" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <limit velocity="6.5" effort="1000" lower="0.0" upper="1.0" />
      <dynamics damping="0.7"/>
    </joint>

    <transmission name="${prefix}_tran_l2">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}_rh_l2">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
      </joint>
      <actuator name="${prefix}_motor_l2">
        <hardwareInterface>PositionJointInterface</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
    </transmission>

    <!-- rh_p12_rn_l2 -->
    <link name="${prefix}_rh_p12_rn_l2">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/l2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
      </collision>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://rh_p12_rn_description/meshes/l2.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="grey"/>
      </visual>

      <inertial>
        <origin xyz="0.000 -0.006 0.011" rpy="0 0 0"/>
        <mass value="0.022"/>
        <!--<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.0" />-->
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
      </inertial>
    </link>

    <gazebo reference="${prefix}_rh_p12_rn_l2">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
      <material>Gazebo/Grey</material>
    </gazebo>

  </xacro:macro>
</robot>
